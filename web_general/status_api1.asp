function DhcpServerRelTime(timeVal)
{
	var timeString = "";
	if (timeVal == 0)
	{
	return '';
	}
	if (timeVal >= 60*60*24)
	{
	timeString += parseInt(timeVal/(60*60*24)) + "" ;
	timeVal %= 60*60*24;
	}
	if (timeVal >= 60*60)
	{
	timeString += parseInt(timeVal / (60*60)) + "" ;
	timeVal %= 60*60;
	}
	if (timeVal >= 60)
	{
	timeString += parseInt(timeVal / (60)) + "" ;
	timeVal %= 60;
	}
	if (timeVal > 0)
	{
	timeString += timeVal + '';
	}
	return timeString;
}
